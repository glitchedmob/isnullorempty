{
    "compilerOptions": {
    "module": "commonjs",
        "target": "ES5",
        "declaration": false,
        "strict": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true
},
    "include": [
    "./src/**/*.ts"
],
    "exclude": [
    "./src/**/*.spec.ts"
]
}
